int removeDuplicates(vector<int>& nums) {
        if(nums.size()<2) return nums.size();
        int cnt=1,unique=0,temp=1;

        while(temp<nums.size()){
            if(nums[unique]==nums[temp]) temp++;
            else{
                swap(nums[++unique],nums[temp++]);
                cnt++;
            }
        }
        return cnt;
}
