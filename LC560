int subarraySum(vector<int>& nums, int k) {
        map<int,int> m;
        int sum=0;
        int cnt=0;
        m.insert({0,1});

        for(int i=0;i<nums.size();i++){
            sum+=nums[i];
            cnt+=m[sum-k];
            m[sum]++;
            // else m.insert({sum,1});
        }

        return cnt;
}
