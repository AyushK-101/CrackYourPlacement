string reverseWords(string s) {
        stringstream ss(s);
        string token="";
        string result="";

        while(getline(ss,token,' ')){
            if(token=="") continue;
            // token[0] = token[0]-32;
            result = token + " " + result;
        }
        if(!result.empty()) result.pop_back();
        return result;
}
